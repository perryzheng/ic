<div class="fields">
<h4>Bullet Point</h4>
<p><%= f.label :header, "Header" %>
	<%= link_to_remove_fields "remove", f%><br />
	<%= f.text_field :header %>
</p>

<%= f.fields_for :bible_verses do |bible_verse| %>
	<%= render 'bible_verse_fields', :f => bible_verse %>
<% end %>
<p class="label"><%= link_to_add_fields "Add One More Bible Verse", f, :bible_verses, "add_fields" %></p>
<hr />
</div>